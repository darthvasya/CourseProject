@{
    Layout = "_AdminPanelLayout";
}

@section Scripts{
    <script src="~/js/adminProducts.js"></script>
}

<div class="panel panel-default">
              <div class="panel-heading main-color-bg">
                <h3 class="panel-title">Инновационные продукты</h3>
              </div>
              <div class="panel-body">
                <div class="row">
                      <div class="col-md-12">
                          <input class="form-control" type="text" placeholder="Введите название продукта или одно из его свойств классификации" id="patternInput">
                      </div>
                </div>
                <br>
                <table class="table table-striped table-hover" id="productsTable">
                </table>
              </div>